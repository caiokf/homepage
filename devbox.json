{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.10.2/.schema/devbox.schema.json",
  "packages": {
    "nodejs": "20.10.0",
    "pnpm": "10.22.0"
  },
  "env": {
    "AWS_CONFIG_FILE": "$PWD/.aws/config",
    "AWS_SHARED_CREDENTIALS_FILE": "$PWD/.aws/credentials",
    "AWS_SDK_LOAD_CONFIG": "true",
    "PATH": "$PWD/node_modules/.bin:$PATH"
  },
  "shell": {
    "init_hook": []
  }
}
